    <SolidColorBrush x:Key="SelectedBorderRowColor" Color="#0078D4"/>
    
    <SolidColorBrush x:Key="ScrollBarThumbColor" Color="#858585"/>
    <SolidColorBrush x:Key="ScrollBarColor" Color="#F0F0F0"/>
    
    <SolidColorBrush x:Key="NavigationColor" Color="#FBFBFB"/>
    <SolidColorBrush x:Key="NavigationBorderColor" Color="#E5E5E5"/>
