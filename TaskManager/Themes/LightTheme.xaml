<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="Primary" Color="#F2F2F2"/>
    <SolidColorBrush x:Key="Secondary" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="Complementary" Color="#F6F6F6"/>
    <SolidColorBrush x:Key="PressedColor" Color="#F9F9F9"/>
    
    <SolidColorBrush x:Key="PrimaryFont" Color="#000"/>
    <SolidColorBrush x:Key="SecondaryFont" Color="#5F5F5F"/>
    <SolidColorBrush x:Key="PressedFontColor" Color="#5F5F5F"/>
    <SolidColorBrush x:Key="DisableFontColor" Color="#A3A3A3"/>
    
    <SolidColorBrush x:Key="HeaderHoverColor" Color="#D9EBF9"/>
    <SolidColorBrush x:Key="HeaderPressedColor" Color="#BCDCF4"/>
    
    <SolidColorBrush x:Key="HeaderBorderColor" Color="#E5E5E5"/>
